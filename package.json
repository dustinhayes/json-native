{
  "name": "json-native",
  "version": "0.0.4",
  "description": "Parse and stringify native javascript types",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dustinhayes/json-native.git"
  },
  "scripts": {
    "test": "jshint . && tape test/*.js",
    "build": "browserify src/index.js --standalone JSONNative | uglifyjs > dist/JSONNative.js"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/31..latest",
      "firefox/31..latest",
      "safari/7..latest",
      "opera/27..latest",
      "iphone/6..latest",
      "ipad/6..latest",
      "android-browser/4.1..latest"
    ]
  },
  "author": "Dustin Hayes",
  "license": "MIT",
  "dependencies": {
    "type-of": "^2.0.1"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "jshint": "^2.6.3",
    "tape": "^3.5.0",
    "uglify-js": "^2.4.16"
  }
}
